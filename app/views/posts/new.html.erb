<h1>Create New Post</h1>

 <%= form_for [@user, @post], html: { multipart: true } do |form| %>
    <div>
        <%= form.label :title %>
        <%= form.text_field :title %>
    </div>
    <div>
        <%= form.label :content %>
        <%= form.text_area :content %>
    </div>
    <%= f.submit "Create Post" %>

  <% end %>
